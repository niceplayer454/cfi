<%--系统公共的Skin--%>

<%--默认的 Edit 操作 Button和HyperLink--%>
<asp:button SkinID="EditPrimary" runat="server" CssClass="primary"></asp:button>
<asp:LinkButton SkinID="EditSecondary" runat="server" CssClass="secondary"></asp:LinkButton>
<tbwc:ReturnLinkButton SkinID="EditSecondary" runat="server" CssClass="secondary"></tbwc:ReturnLinkButton>
<asp:HyperLink SkinID="EditSecondary" runat="server" CssClass="secondary"></asp:HyperLink>


<%--默认的 只读TextBox--%>
<asp:TextBox SkinID="ReadonlyTxt" runat="server" ToolTip="ReadOnly" CssClass="readonly"></asp:TextBox>

<%--默认的 只读ToolButton--%>
<tbwc:ToolButton SkinID="New" runat="server" ><LinkStyle CssClass="tbNew" /></tbwc:ToolButton>
<tbwc:ToolButton SkinID="Search" runat="server" ><LinkStyle CssClass="tbNew" /></tbwc:ToolButton>
<tbwc:ToolButton SkinID="Fresh" runat="server" ><LinkStyle CssClass="tbFresh" /></tbwc:ToolButton>


<%--默认的 DatePicker--%>
<tbwc:DatePicker runat="server" CssClass="datePicker" SplitChar="-" ImagesFolder="~/themes/Default/DatePicker/images/" />
<tbwc:LabelDatePicker runat="server" CssClass="datePicker" SplitChar="-" ImagesFolder="~/themes/Default/DatePicker/images/" />

<%--默认的 SearchPanel--%>
<tbwc:SearchPanel runat="server" Collapsed="True" CssClass="search" SwitchImageUrl="~/themes/Default/Searcher/collapse.jpg"
    SwitchCssClass="switch" BarCssClass="bar" ContainerCssClass="container" OpCssClass="op"></tbwc:SearchPanel>

<%--dataList--%>
<%--List--%>
<tbwc:GridViewEx SkinID="List" runat="server"></tbwc:GridViewEx>
