<%@ Register Assembly="TB.Web.Common" Namespace="TB.Web.UI.WebControls" TagPrefix="tbwc" %>
<%@ Register Assembly="TB.Web.DataControl" Namespace="TB.Web.UI.WebControls" TagPrefix="tbwc" %>
<%@ Register Assembly="TB.Web.UI.DatePicker" Namespace="TB.Web.UI.WebControls" TagPrefix="tbwc" %>
<%@ Register Assembly="TB.Web.UI.DatePicker" Namespace="TB.Web.UI.WebControls.Searcher" TagPrefix="tbwc" %>
